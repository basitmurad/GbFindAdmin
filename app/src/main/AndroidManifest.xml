<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.MANAGE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission
        android:name="android.permission.READ_MEDIA_IMAGES"
        tools:ignore="SelectedPhotoAccess" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GbFindAdmin"
        tools:targetApi="31">
        <activity
            android:name=".DeliverActivity"
            android:exported="false" />
        <activity
            android:name=".ProgressActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.OrderActivity"
            android:exported="false" />
        <activity
            android:name=".OrderActivity"
            android:exported="false" />
        <activity
            android:name=".OrderShowActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.CreateVendorActivity"
            android:exported="false" />
        <activity
            android:name=".VendorDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".VendorDetailActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.AddItemActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.DashboardActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".vendorMode.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>